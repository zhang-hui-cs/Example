{
  "name": "cesium.js",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://cesium.com/docs/cesiumjs-ref-doc/*"],
      "js": ["content.js"],
      "css": ["cesium.doc.css"]
    }
  ],
  "browser_action": {
    "default_title": "cesium-css"
  },
  "icons":{
    "16": "cesium.png",
    "48": "cesium.png",
   "128": "cesium.png"
  },
  "permissions": ["tabs"]
}
